{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "commonjs",
    "outDir": "dist",
    "target": "ES6",
    "baseUrl": ".",
    "noImplicitAny": true,
    "esModuleInterop": true,
        "paths": {
            "*": [
                "node_modules/*",
                "src/types/*"
            ]
        }
    },
    "include": [
        "src/**/*"
    ]
}
