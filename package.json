{
  "name": "gryphonbot",
  "version": "3.2.4",
  "description": "Chatbot for discord",
  "main": "./lib/app.js",
  "author": "Ale≈° Olesov",
  "license": "MIT",
  "scripts": {
    "start": "nodemon lib/app.js --exec babel-node",
    "build": "babel lib -d dist",
    "serve": "node dist/app.js",
    "test": "cross-env NODE_ENV=test nyc mocha --compilers=js:babel-core/register --require=babel-polyfill --reporter=lcovonly -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "MALjs": "^0.1.0",
    "babel-plugin-inline-dotenv": "1.1.2",
    "booru": "^0.3.3",
    "cleverbot-node": "^0.3.11",
    "codacy-coverage": "2.0.3",
    "discord.js": "^11.2.1",
    "dotenv": "5.0.0",
    "ffmpeg-binaries": "3.2.2-3",
    "firebase-admin": "5.8.2",
    "google-translate-api": "^2.3.0",
    "got": "8.0.1",
    "he": "^1.1.1",
    "jimp": "^0.2.28",
    "mocha-lcov-reporter": "1.3.0",
    "opusscript": "0.0.3",
    "random-natural": "^1.0.3",
    "relevant-urban": "^2.0.0",
    "sfwbooru": "0.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.3",
    "babel-plugin-istanbul": "4.1.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "cross-env": "5.1.3",
    "istanbul": "0.4.5",
    "mocha": "5.0.1",
    "nyc": "11.4.1",
    "standart": "6.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/Peskyn12/griffin-bot.git"
  },
  "keywords": [
    "chatbot",
    "discord"
  ],
  "homepage": "https://bitbucket.org/Pesky12/griffin-bot#readme"
}
